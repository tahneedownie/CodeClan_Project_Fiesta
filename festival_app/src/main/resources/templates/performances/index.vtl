<p class="pageHeader">List of Performances</p>

<table>
    <tr>
        <th>Time</th>
        <th>Duration</th>
        <th>Line-Up (date)</th>
        <th>Venue</th>
    </tr>


    #foreach ($performance in $performances)
    <tr>
        <td>$performance.getTime()</td>
        <td>$performance.getDuration()</td>
        <td>$performance.getLineUp().getDate()</td>
        <td>$performance.getLineUp().getVenue().getName()</td>
    <tr>
        #end

</table>


