<html>
<head>
    <link href="https://fonts.googleapis.com/css?family=Lobster|Open+Sans" rel="stylesheet">
    <link rel="stylesheet" href="/style.css">
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Venues</title>
</head>
<body>
<img class= "icon" src="/images/fest_app_venue_icon_vivid_tangelo.png" alt="artist icon">
<h4>Venues</h4>

<table>
    <tr>
        <th>Name</th>
        <th>Location</th>
        <th>Capacity</th>
    </tr>


    #foreach ($venue in $venues)
    <tr>
        <td><a href="/venues/$venue.getId()">$venue.getName()</a></td>
        <td>$venue.getLocation()</td>
        <td>$venue.getVisitorCapacity()</td>
    <tr>
        #end

</table>

<a class="add_venue" href="/venues/new"> Add Venue </a>
</body>
</html>














