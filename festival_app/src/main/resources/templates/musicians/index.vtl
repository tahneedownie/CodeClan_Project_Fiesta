<p class="pageHeader">All Musicians</p>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Manager</th>
        <th>Genre</th>
    </tr>


    #foreach ($musician in $musicians)
    <tr>
        <td>$musician.getFirstName()</td>
        <td>$musician.getLastName()</td>
        <td>$musician.getManager()</td>
        <td>$musician.getGenre()</td>
    <tr>
        #end

</table>


